page{
  width: 100%;
  height: 100%;
}
.scrollView{
  width: 100%;
  height: 100%;
  background-color: #f3f3f3;
}
.bgView{
  width: 100%;
  height: 170rpx;
  margin: 10rpx auto;
  position: relative;
  background-color: white;
}
.image{
  height: 100rpx;
  width: 150rpx;
  margin-top: 8rpx;
  margin-left: 20rpx;
}
.storeName{
  position: absolute;
  top: 8rpx;
  left: 185rpx;
  font-size: 25rpx;
  color: black;
}
.orderId{
  position: absolute;
  top: 70rpx;
  left: 185rpx;
  font-size: 25rpx;
  color: gray;
}
.line{
  width: 95%;
  height: 1px;
  background-color: #f3f3f3;
  margin: -5rpx auto;
}
.projectName{
  font-size: 25rpx;
  color: black;
  margin-left: 20rpx;
}
.price{
  position: absolute;
  bottom: 10rpx;
  right: 20rpx;
  font-size: 25rpx;
  color: #39b5de;
}
.num{
  position: absolute;
  bottom: 10rpx;
  right: 120rpx;
  font-size: 25rpx;
  color: gray;
}
.class_item_end{
  border-bottom: solid #f5f5f5 1px;
  padding: 20px;
  text-align: center;
}